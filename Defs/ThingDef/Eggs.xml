	<ThingDef ParentName="EggFertBase">
        <defName>EggBrachyuraCoastalFertilizedMale</defName>
        <label>Brachyura Fertilized Male Egg</label>
        <description>A soon to be juvenile brahcyura Male.</description>
        <graphicData>
          <texPath>Resources/BrachyuraEggFertilized/CoastalMale</texPath>
          <drawSize>1</drawSize>
        </graphicData>
		<stackLimit>1</stackLimit>
		<tradeability>None</tradeability>
	    <statBases>
		  <Beauty>1</Beauty>
		  <Mass>0.15</Mass>
		  <Flammability>1.0</Flammability>
		  <MarketValue>500</MarketValue>
	    </statBases>
        <comps>
         <li Class="HatchAsPlayerFaction.CompProperties_HatcherPlayer">
           <hatcherDaystoHatch>30</hatcherDaystoHatch>
           <hatcherPawn>pphhyy_Brachyura_Male</hatcherPawn>
         </li>
		 <li Class="CompProperties_Rottable">
			<daysToRotStart>60</daysToRotStart>
			<rotDestroys>true</rotDestroys>
			<disableIfHatcher>true</disableIfHatcher>
		  </li>
        </comps>
     </ThingDef>

